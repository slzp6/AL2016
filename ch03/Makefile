	$(CC) $(CFLAGS) $(LIBS) ex3-6.c -o ex3-6

ex3-7: ex3-7.c
	$(CC) $(CFLAGS) $(LIBS) ex3-7.c -o ex3-7

ex3-8: ex3-8.c
	$(CC) $(CFLAGS) $(LIBS) ex3-8.c -o ex3-8

