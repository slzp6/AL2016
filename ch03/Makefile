	$(CC) $(CFLAGS) $(LIBS) ex3-9.c -o ex3-9

q3-1: q3-1.c
	$(CC) $(CFLAGS) $(LIBS) q3-1.c -o q3-1

q3-2: q3-2.c
	$(CC) $(CFLAGS) $(LIBS) q3-2.c -o q3-2

