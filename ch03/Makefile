	$(CC) $(CFLAGS) $(LIBS) ex3-3.c -o ex3-3

ex3-4: ex3-4.c
	$(CC) $(CFLAGS) $(LIBS) ex3-4.c -o ex3-4

ex3-5: ex3-5.c
	$(CC) $(CFLAGS) $(LIBS) ex3-5.c -o ex3-5

